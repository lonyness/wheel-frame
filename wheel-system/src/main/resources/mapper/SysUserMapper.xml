<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frame.wheel.wheelsystem.dao.SysUserMapper">
    <resultMap id="result" type="com.frame.wheel.wheelsystem.entity.User">
        <id column="user_id" property="userId" />
        <result column="name" property="name" />
    </resultMap>
    <select id="getOneByName" resultType="com.frame.wheel.wheelsystem.entity.User" resultMap="result" >
        select * from sys_user where name = #{name}
    </select>


</mapper>